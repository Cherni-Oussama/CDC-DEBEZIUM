{
  "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
  "connection.url": "jdbc:postgresql://postgres-sink:5433/postgres",
  "connection.user": "postgres",
  "connection.password": "password",
  "tasks.max": "1",
  "topics": "CDC-AVRO.public.employee,CDC-AVRO.public.company",
  "transforms": "unwrap,changeTopicName",
  "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
  "transforms.changeTopicName.replacement": "$1",
  "transforms.changeTopicName.type": "org.apache.kafka.connect.transforms.RegexRouter",
  "transforms.changeTopicName.regex": "1.public.(.*)",
  "transforms.unwrap.drop.tombstones": "false",
  "key.converter.schemas.enable": "false",
  "auto.evolve": "true",
  "auto.create": "true"
}